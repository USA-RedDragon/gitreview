{
    "extends": [
        "plugin:vue/essential",
        "google",
        "eslint:recommended"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "object-curly-spacing": [ "error", "always" ],
        "require-jsdoc": "off",
        "indent": [ "error", 4, { "SwitchCase": 1 } ],
        "max-len": [ "error", 120 ],
        "no-unused-vars": [ "error", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" } ],
        "valid-jsdoc": "off"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    }
}